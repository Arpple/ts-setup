{
  "name": "ts-setup",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:prod": "node bin/index.js",
    "test": "ava",
    "coverage": "nyc ava",
    "lint": "tslint --format stylish src/**/*.ts",
    "clean": "rm -rf dist/*",
    "build": "tsc -p tsconfig.json",
    "prebuild": "yarn clean",
    "build:prod": "tsc -p tsconfig.prod.json",
    "prebuild:prod": "yarn clean",
    "build:debug": "tsc -p tsconfig.json",
    "sandbox": "ts-node ./sandbox.ts"
  },
  "keywords": [],
  "author": "Arpple",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^10.11.3",
    "ava": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "prettier": "^1.18.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-immutable": "^5.0.0",
    "typescript": "^3.4.5"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "cache": false,
    "sourceMap": true
  }
}
