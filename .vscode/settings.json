{
    "fileswitcher.mappings": [
        {
            "from": "src/(.+)\\.(.+)",
            "to": "src/:1.test.:2"
        },
        {
            "from": "src/(.+)\\.test\\.(.+)",
            "to": "src/:1.:2"
        },
    ],
    "createTests.template.default": [
        "import test from \"ava\"",
        "",
        "test(\"\", (assert) => {",
        "",
        "})"
    ],
    "tslint.autoFixOnSave": true
}